Options -Indexes

<IfModule mod_rewrite.c>
RewriteEngine On

# Respect existing files and directories
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Route everything else to the front controller
# Language-prefixed paths like /en/products will be handled in index.php
RewriteRule ^ index.php [QSA,L]
</IfModule>


